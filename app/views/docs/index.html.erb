<p id="notice"><%= notice %></p>
<h1>Docs</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Resumen</th>
      <th>Texto</th>
      <th>Fecha</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @docs.each do |doc| %>
      <tr>
        <td><%= doc.titulo %></td>
        <td><%= doc.resumen %></td>
        <td><%= doc.texto %></td>
        <td><%= doc.fecha %></td>
        <td><%= button_to 'Show', doc, :method => :get %></td>
        <td><%= button_to 'Edit', edit_doc_path(doc), :method => :get%></td>
        <td><%= button_to 'Destroy', doc, method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Doc', new_doc_path %>
